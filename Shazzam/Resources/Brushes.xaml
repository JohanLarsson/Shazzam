        <GradientStop Offset="0.65" Color="#1000" />
        <GradientStop Offset="1" Color="#3000" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ErrorBrush" StartPoint="0,0" EndPoint="0,1">
