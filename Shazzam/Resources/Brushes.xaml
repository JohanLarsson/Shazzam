            <DrawingGroup>
                <GeometryDrawing Brush="{StaticResource DarkBackgroundBrush}">
                    <GeometryDrawing.Geometry>
                        <GeometryGroup>
                            <RectangleGeometry Rect="0 0 6 6" />
                            <RectangleGeometry Rect="6 6 6 6" />
                        </GeometryGroup>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingBrush.Drawing>
    </DrawingBrush>

